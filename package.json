{
  "name": "nanoquery",
  "version": "0.0.1",
  "type": "module",
  "description": "A small library for parsing and serialisation query strings",
  "main": "lib/main.js",
  "files": [
    "lib",
    "!lib/test"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npm run build && node --test lib/test/**/*_test.js",
    "lint": "eslint src",
    "format": "prettier --write src",
    "bench": "node ./bench/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/es-tooling/nanoquery.git"
  },
  "keywords": [
    "qs",
    "querystring",
    "query",
    "query-string",
    "url"
  ],
  "author": "James Garbutt (https://github.com/43081j)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/es-tooling/nanoquery/issues"
  },
  "homepage": "https://github.com/es-tooling/nanoquery#readme",
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@types/node": "^20.12.7",
    "c8": "^9.1.0",
    "prettier": "^3.2.5",
    "tinybench": "^2.8.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1"
  },
  "dependencies": {
    "fast-querystring": "^1.1.2"
  }
}
